# GeoBlinko Environment Variables Configuration
# Copy this file to .env and update with your actual values

# NextAuth Configuration
NEXTAUTH_SECRET=your_secure_nextauth_secret_here

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# Amap API Key (Required for location features)
# Get your key from: https://console.amap.com/
AMAP_WEB_API_KEY=your_amap_api_key_here
